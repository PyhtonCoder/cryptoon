def dat():
    a = []
    for i in range(5):
        num = int(input("Ներմուծել թիվը: "))
        a.append(num)
        if i>5:
            break
        else:
            continue

    print(a)

dat()

# Ֆունկցիա, որը ներմուծում է թվեր, և ներմուծված թվերը տեղադրում է a զանգվածում: կարելի է ներմուծել մինչև 5 հատ:
